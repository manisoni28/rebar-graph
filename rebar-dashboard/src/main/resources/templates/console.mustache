
{{>header}}

        <!-- page content -->
        <div class="right_col" role="main">
          
        
          
   

	    
          <!-- row -->
               <div class="row">
              <div class="col-md-12 col-sm-12 col-xs-12">
                         {{#errorMessage}}
	    <div class="alert alert-error" style="display:block">
	       <ul class="fa-ul">
	          <li> <i class="fa fa-info-circle fa-lg fa-li"></i>{{.}}</li>
	       </ul>
	    </div>
	    {{/errorMessage}}
	    

              <div class="x_panel" >
      <div class="x_content">    
<form method="POST" action="/exec">
    <label for="cypher">Cypher: </label>
                          <textarea id="cypher" name="cypher" required="required" class="form-control"  data-parsley-trigger="keyup" 
                          data-parsley-minlength="20" data-parsley-maxlength="100" 
                          data-parsley-minlength-message=""
                            data-parsley-validation-threshold="10">{{cypher}}</textarea>

                          <br/>
                       
                  <div class="form-group">
                        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
              
                          <button type="submit" class="btn btn-success">Submit</button>
                        </div>
                      </div>
                          </form>
            </div>
            
            </div></div></div>
            

	    {{#results}}
             <div class="row">
              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Result</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                
                      <li><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <p class="text-muted font-13 m-b-30">
                     
                    </p>
                    <table id="datatable" class="table table-striped table-bordered" data-page-length="100">
                      <thead>
                        <tr>
                        {{#columnNames}}
                        <th>{{.}}</th>
                        {{/columnNames}}
                         
                        </tr>
                      </thead>


                      <tbody>
                      {{#rows}}
                        <tr>
                         {{#vals}}
                         <td>{{this}}</td>
                         {{/vals}}
                    
                    
                        </tr>
                        {{/rows}}
                       
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
              
              </div>
            {{/results}}
            
        </div>
        <!-- /page content -->
{{>footer}}
